{% if parameter.IsStringArray -%}
request.Headers.TryAddWithoutValidation("{{ parameter.Name }}", {{ parameter.VariableName }});
{% elsif parameter.IsDateTime -%}
request.Headers.TryAddWithoutValidation("{{ parameter.Name }}", ConvertToString({{ parameter.VariableName }}{% if parameter.IsNullable %}?{% endif %}.ToString("{{ ParameterDateTimeFormat }}"), CultureInfo.InvariantCulture));
{% else -%}
request.Headers.TryAddWithoutValidation("{{ parameter.Name }}", ConvertToString({{ parameter.VariableName }}, CultureInfo.InvariantCulture));
{%- endif %}
